    "devDependencies": {
        "@parcel/transformer-sass": "^2.8.2",
        "parcel": "latest",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@parcel/transformer-postcss": "^2.8.2",
        "autoprefixer": "^10.4.13",
        "yarn": "^1.22.19"
    }
}
